{
  "name": "react-furi",
  "version": "0.0.0-development",
  "description": "React Furigana component",
  "license": "MIT",
  "author": {
    "name": "Duncan Bay",
    "email": "duncan.bay@gmail.com"
  },
  "repository": "https://github.com/DJTB/react-furi.git",
  "homepage": "https://djtb.github.io/react-furi",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "nps --no-scripts",
    "precommit": "remove-lockfiles && lint-staged",
    "test": "nps test"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "styled-components": "3.2.6"
  },
  "dependencies": {
    "cuid": "2.1.1",
    "just-zip-it": "2.0.0",
    "wanakana": "4.0.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.5.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-styled-components": "5.0.1",
    "lint-staged": "^4.2.3",
    "nps": "^5.7.1",
    "nps-utils": "^1.4.0",
    "prettier": "^1.7.4",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "styled-components": "3.2.6",
    "remove-lockfiles": "^1.4.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.fixture.js"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "japanese",
    "furigana",
    "ruby",
    "react"
  ]
}
