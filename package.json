  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.js", "!src/demo/*.*"],
    "transformIgnorePatterns": ["node_modules/(?!(wanakana)/)"]
